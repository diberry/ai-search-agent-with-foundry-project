{
  "name": "azure-search-agentic-retrieval-quickstart",
  "version": "1.0.0",
  "description": "Azure AI Search agentic retrieval quickstart with TypeScript",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node --env-file ./.env dist/index.js",
    "dev": "tsc && node --env-file ./.env dist/index.js",
    "upload": "tsc && node --env-file ./.env dist/upload.js",
    "dev:js": "node --env-file ./.env src/JS-ONLY/index.js",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "azure",
    "search",
    "agentic-retrieval",
    "typescript",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@azure/identity": "^4.5.0",
    "@azure/search-documents": "^12.3.0-beta.1"
  },
  "devDependencies": {
    "@types/node": "^22.19.3",
    "typescript": "^5.7.2"
  }
}
